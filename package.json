{
  "name": "neobb",
  "version": "0.0.1",
  "description": "Modern internet forum script",
  "author": "svantetic",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "nest": "./node_modules/@nestjs/cli/bin/nest.js",
    "prebuild": "rm -rf dist",
    "build": "nest build && tailwind build src/frontend/css/app.css -o public/css/app.css && fe:prod",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:dev:fe": "concurrently -k \"yarn:start:dev\" \"yarn:fe:dev\"",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node app.js",
    "fe:dev": "node_modules/webpack/bin/webpack.js --config webpack.config.js --mode development",
    "fe:dev:watch": "fe:dev --watch",
    "fe:prod": "node_modules/webpack/bin/webpack.js --config webpack.config.js --mode production",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/config/typeorm.ts",
    "typeorm:migrate": "npm run typeorm migration:generate -- -n",
    "typeorm:run": "npm run typeorm migration:run"
  },
  "dependencies": {
    "@babel/preset-react": "^7.10.1",
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/platform-socket.io": "^7.3.1",
    "@nestjs/typeorm": "^7.1.0",
    "@nestjs/websockets": "^7.3.1",
    "@types/axios": "^0.14.0",
    "@types/connect-flash": "^0.0.35",
    "@types/express-session": "^1.17.0",
    "@types/passport": "^1.0.3",
    "@types/react": "^16.9.36",
    "@types/react-dom": "^16.9.8",
    "@types/socket.io-client": "^1.4.33",
    "axios": "^0.19.2",
    "babel-loader": "^8.1.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.2.3",
    "connect-flash": "^0.1.1",
    "express-session": "^1.17.1",
    "faker": "^4.1.0",
    "hbs": "^4.1.1",
    "joi": "^14.3.1",
    "mysql": "^2.18.1",
    "nunjucks": "^3.2.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "sass-loader": "^8.0.2",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "source-map-loader": "^1.0.0",
    "tailwindcss": "^1.4.6",
    "typeorm": "^0.2.25",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^8.5.0",
    "vue-router": "^3.3.4",
    "vuetify": "^2.3.3",
    "vuex": "^3.5.1",
    "vuex-class": "^0.3.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.4.1",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/express": "^4.17.3",
    "@types/jest": "25.2.3",
    "@types/node": "^13.9.1",
    "@types/socket.io": "^2.1.8",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "concurrently": "^5.2.0",
    "css-loader": "^3.5.3",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "26.0.1",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-loader": "^3.0.0",
    "postcss-scss": "^2.1.1",
    "prettier": "^1.19.1",
    "sass": "^1.26.7",
    "supertest": "^4.0.2",
    "ts-jest": "26.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4",
    "vue-loader": "^15.9.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
